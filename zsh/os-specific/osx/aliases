alias ds='sudo dscacheutil -flushcache;sudo killall -HUP mDNSResponder;'
alias wpeh="~/dev/wpe/artisan"
alias date="gdate"
alias df="gdf"
alias du="gdu"

# file types
alias -s md=code
alias -s json=code
alias -s {css,scss,ts,js,jsx,html}=code

# php
alias composer8.0="php8.0 `which composer`"
alias composer8.1="php8.1 `which composer`"
